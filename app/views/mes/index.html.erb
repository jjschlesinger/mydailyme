<% for me in @mes %>
<div class="editSelect">
<b><%=h me.title %></b>
<%= link_to 'Show', me %>
<%= link_to 'Edit', edit_me_path(me) %>
<%= link_to 'Destroy', me, :confirm => 'Are you sure?', :method => :delete %>
<%= link_to 'Invite', :controller => :invites, :action => :new, :id=>me.id %>
<br />
<%=h me.subscribe_token %>
</div>
<% end %>


<br />

<%= link_to 'New me', new_me_path %>
