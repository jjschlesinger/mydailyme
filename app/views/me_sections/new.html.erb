<h1>New me_section</h1>

<%= error_messages_for :me_section %>

<% form_for(@me_section) do |f| %>
  <p>
    <b>Section</b><br />
    <%= f.text_field :section_id %>
  </p>

  <p>
    <b>Value</b><br />
    <%= f.text_field :value %>
  </p>

  <p>
    <b>Other name</b><br />
    <%= f.text_field :other_name %>
  </p>

  <p>
    <b>Me</b><br />
    <%= f.text_field :me_id %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', me_sections_path %>
